{
        "ocaml.sandbox": {
                "kind": "opam",
                "switch": "4.14.2"
        },
        "files.associations": {
                "smmintrin.h": "c",
                "tmmintrin.h": "c",
                "pmmintrin.h": "c",
                "*.tbl": "c",
                "platform.h": "c",
                "optional": "c",
                "system_error": "c",
                "array": "c",
                "functional": "c",
                "tuple": "c",
                "type_traits": "c",
                "utility": "c"
        },
        "[ocaml]": {
                "editor.defaultFormatter": "ocamllabs.ocaml-platform",
        },
        "[ocaml.interface]": {
                "editor.defaultFormatter": "ocamllabs.ocaml-platform",
        },
        "makefile.configureOnOpen": false,
}
