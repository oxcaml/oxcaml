{
        "files.associations": {
                "smmintrin.h": "c",
                "tmmintrin.h": "c",
                "pmmintrin.h": "c",
                "*.tbl": "c",
                "platform.h": "c"
        },
        "[ocaml]": {
                "editor.defaultFormatter": "ocamllabs.ocaml-platform",
        },
        "[ocaml.interface]": {
                "editor.defaultFormatter": "ocamllabs.ocaml-platform",
        },
        "makefile.configureOnOpen": false,
        "ocaml.sandbox": {
                "kind": "custom",
                "template": "OPAMROOT=/usr/local/home/$USER/.opam opam exec --switch=4.14.2 --set-switch -- $prog $args"
        },
}
